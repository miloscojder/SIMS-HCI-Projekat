<Window x:Class="Projekat.DoctorAssessment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekat"
        mc:Ignorable="d"
        Title="DoctorAssessment" Height="450" Width="800">
    <Grid>
        <DataGrid Margin="0,0,0,255" x:Name="doctorAssessmentDataGrid" AutoGenerateColumns="False" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="Doctor username"
                    Binding="{Binding Username}" />
                <DataGridTextColumn Header="Assessment"
                    Binding="{Binding Grade}" />


            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
