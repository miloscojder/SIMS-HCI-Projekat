<Window x:Class="Projekat.proba"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekat"
        xmlns:chart="clr-namespace:Syncfusion;assembly=Syncfusion.Licensing"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        d:DesignHeight="300" d:DesignWidth="300" d:DataContext="{d:DesignInstance local:proba}"
        Title="proba" Height="450" Width="800">
    <Grid>
        <lvc:CartesianChart Series="{Binding}" ></lvc:CartesianChart>
    </Grid>
</Window>
